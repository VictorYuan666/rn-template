{
  "name": "rn-template",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android:shake": "adb shell input keyevent 82",
    "icon": "npx iconfont-rn",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:noCache": "react-native start --reset-cache",
    "eslint": "eslint --fix --ext .ts,.tsx src/",
    "prettier": "prettier --check src/**/*.tsx Â·--write",
    "postinstall": "husky install",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ios/build",
    "clean": "react-native-clean-project",
    "cz": "git-cz",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "test": "jest --passWithNoTests --detectOpenHandles --watch",
    "e2e:ios:test": "detox test -c ios",
    "e2e:ios:build": "detox build -c ios",
    "e2e:android:test": "detox test -c android",
    "e2e:android:build": "detox build -c android",
    "set:icon": "react-native set-icon --path ./assets/logo.png --background \"#FFFFFF\"",
    "set:splash": "yarn react-native generate-bootsplash assets/bootsplash_logo.png --background-color=ffffff --logo-width=100",
    "set": "run-p set:*",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog-emoji"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.16",
    "@react-native-community/datetimepicker": "^5.1.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^7.1.7",
    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "ahooks": "^3.1.8",
    "axios": "^0.25.0",
    "color": "^4.2.0",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "hox": "^1.1.5",
    "i18next": "^21.6.7",
    "immer": "^9.0.12",
    "lodash-es": "^4.17.21",
    "react": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.15.3",
    "react-native": "^0.67.1",
    "react-native-bootsplash": "^4.1.0",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.8",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.3.1",
    "react-native-redash": "16.2.3",
    "react-native-restart": "^0.0.23",
    "react-native-root-siblings": "^4.1.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-svg": "^12.1.1",
    "react-native-ui-lib": "^6.8.1",
    "use-immer": "^0.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/runtime": "^7.16.7",
    "@bam.tech/react-native-make": "^3.0.3",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash-es": "^4.17.5",
    "@types/react-native": "^0.66.15",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@umijs/fabric": "^2.10.1",
    "babel-jest": "^27.4.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "detox": "^19.4.2",
    "eslint": "^8.7.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.6",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.2.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm-run-all": "^4.1.5",
    "pont-engine": "1.0.13",
    "prettier": "^2.5.1",
    "react-native-clean-project": "^4.0.0",
    "react-native-iconfont-cli": "^2.2.4",
    "react-test-renderer": "^17.0.2",
    "shelljs": "^0.8.5",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
