{
  "name": "rn-template",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android:shake": "adb shell input keyevent 82",
    "icon": "npx iconfont-rn",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:noCache": "react-native start --reset-cache",
    "eslint": "eslint --fix --ext .ts,.tsx src/",
    "prettier": "prettier --check src/**/*.tsx --write",
    "postinstall": "husky install",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ios/build",
    "clean": "run-p clean:*",
    "cz": "git-cz",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "test": "jest --passWithNoTests --detectOpenHandles --watch",
    "e2e:ios:test": "detox test -c ios",
    "e2e:ios:build": "detox build -c ios",
    "e2e:android:test": "detox test -c android",
    "e2e:android:build": "detox build -c android",
    "set:icon": "react-native set-icon --path ./assets/logo.png --background \"#FFFFFF\"",
    "set:splash": "yarn react-native generate-bootsplash assets/bootsplash_logo.png --background-color=ffffff --logo-width=100",
    "set": "run-p set:*",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog-emoji"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-native-picker/picker": "^2.1.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "ahooks": "^2.10.11",
    "axios": "^0.21.4",
    "color": "^4.0.1",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "hox": "^1.1.4",
    "i18next": "^21.3.3",
    "immer": "^9.0.6",
    "lodash-es": "^4.17.21",
    "react": "17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-i18next": "^11.12.0",
    "react-native": "^0.66.1",
    "react-native-bootsplash": "^3.2.6",
    "react-native-config": "^1.4.4",
    "react-native-device-info": "^8.4.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.3.0-beta.3",
    "react-native-redash": "16.2.2",
    "react-native-restart": "^0.0.22",
    "react-native-root-siblings": "^4.1.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-svg": "^12.1.1",
    "react-native-ui-lib": "^6.2.0",
    "use-immer": "^0.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "@bam.tech/react-native-make": "^3.0.3",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^8.0.0",
    "@types/detox": "^17.14.3",
    "@types/jest": "^27.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/react-native": "^0.66.1",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-test-renderer": "^17.0.1",
    "@umijs/fabric": "^2.8.1",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "detox": "^18.23.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.2.4",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm-run-all": "^4.1.5",
    "pont-engine": "1.0.13",
    "prettier": "^2.4.1",
    "react-native-iconfont-cli": "^2.2.4",
    "react-test-renderer": "^17.0.2",
    "shelljs": "^0.8.4",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
